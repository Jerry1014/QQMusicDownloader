<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>com.Search</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/S</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoadLive2d</servlet-name>
        <servlet-class>com.LoadLive2d</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadLive2d</servlet-name>
        <url-pattern>/L</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetSongLrcAndPlaySrc</servlet-name>
        <servlet-class>com.GetSongLrcOrPlaySrc.GetSongLrcAndPlaySrc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetSongLrcAndPlaySrc</servlet-name>
        <url-pattern>/P</url-pattern>
    </servlet-mapping>
</web-app>